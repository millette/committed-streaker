{
  "name": "committed-streaker",
  "version": "0.9.1",
  "description": "We salute the streaker in you.",
  "license": "AGPL-3.0",
  "repository": "millette/committed-streaker",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "bower": "bower install",
    "css": "gulp sass",
    "lint": "standard",
    "docker": "docker build -t millette/committed-streaker:$npm_package_version .",
    "dev": "standard && DEBUG=app gulp",
    "start": "standard && gulp sass && node ./bin/www",
    "test": "standard && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "committed-streaker": "bin/www"
  },
  "files": [
    "app.js",
    "lib/passport.js",
    "routes",
    "views",
    "lib",
    "public/css/style.css",
    "public/img",
    "public/js",
    "bin"
  ],
  "keywords": [
    "streaking",
    "commits",
    "rollodeqc",
    "github",
    "gh",
    "user",
    "username",
    "info"
  ],
  "dependencies": {
    "body-parser": "^1.15.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.2",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "express-session-level": "^1.0.0",
    "express-session-passport-cleanup": "^1.0.3",
    "level": "^1.4.0",
    "lodash.sortby": "^4.5.0",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
