    "bower": "^1.7.9",
    "coveralls": "^2.11.9",
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.0.7",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.1",
    "nyc": "^6.4.4",
    "standard": "^7.1.0"
  }
}
